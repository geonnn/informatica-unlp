programa R-info_Gonzalo
areas
  ciudad: AreaC(1, 1, 100, 100)
robots
  robot robot1
  comenzar
    Pos(65, 50)
    repetir 3
      derecha
    repetir 42
      si HayPapelEnLaBolsa
        depositarPapel
      mover
    si HayPapelEnLaBolsa
      depositarPapel
    Informar(PosAv, PosCa)
  fin
variables
  R-info: robot1
comenzar
  AsignarArea(R-info, ciudad)
  Iniciar(R-info, 1, 1)
fin
